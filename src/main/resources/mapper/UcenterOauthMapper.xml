<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zlcm.server.dao.user.UcenterOauthDao">
    <insert id="save" parameterType="com.zlcm.server.model.user.UcenterOauth">
       INSERT INTO ucenter_oauth(oauth_id, name) VALUES (#{oauth_id,jdbcType=INTEGER},#{name,jdbcType=VARCHAR})
    </insert>

    <update id="update" parameterType="com.zlcm.server.model.user.UcenterOauth">
      update ucenter_oauth
      set name = #{name,jdbcType=VARCHAR}
      where oauth_id = #{oauth_id,jdbcType=VARCHAR}
    </update>

    <delete id="deleteByPK" parameterType="java.lang.Integer">
       DELETE FROM ucenter_oauth WHERE oauth_id = #{pk,jdbcType=VARCHAR}
    </delete>
    <select id="get" parameterType="java.lang.Integer" resultType="com.zlcm.server.model.user.UcenterOauth">
      SELECT * FROM ucenter_oauth WHERE oauth_id = #{pk,jdbcType=VARCHAR}
    </select>
    <select id="findAll" resultType="com.zlcm.server.model.user.UcenterOauth">
        select *
        from ucenter_oauth;
    </select>
    <select id="getCount" resultType="java.lang.Integer">
        select count(*)
        from ucenter_oauth;
    </select>
    <select id="getPageList" resultType="java.lang.Integer">
        select *
        from ucenter_oauth LIMIT #{page,jdbcType=INTEGER},#{size,jdbcType=INTEGER}
    </select>
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
</mapper>